<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.submission.teacher.mapper.HomeworkMapper">

    <resultMap id="BaseResultMap" type="com.submission.teacher.entity.HomeworkBase">
            <id property="homeworkId" column="homework_id" jdbcType="VARCHAR"/>
            <result property="homeworkName" column="homework_name" jdbcType="VARCHAR"/>
            <result property="homeworkDescription" column="homework_description" jdbcType="VARCHAR"/>
            <result property="totalPoint" column="total_point" jdbcType="INTEGER"/>
            <result property="courseId" column="course_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        homework_id,homework_name,homework_description,
        total_point,course_id
    </sql>
</mapper>
